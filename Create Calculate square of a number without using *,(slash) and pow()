#include <stdio.h>
int square(int n) {
    int result = 0;
    for (int i = 0; i < n; i++) {
        result += n;
    }
    return result;
}
int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);
    int sq = square(num);
    printf("Square of %d is: %d\n", num, sq);
    return 0;
}
